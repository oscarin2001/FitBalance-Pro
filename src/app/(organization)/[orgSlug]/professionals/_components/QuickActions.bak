"use client";
import React from "react";
import Link from "next/link";
import { Button } from "@/components/ui/button";
import CreateCoachForm from "../coach/_components/CreateCoachForm";
import CreateNutritionistForm from "../nutritionist/_components/CreateNutritionistForm";
import { ProfessionalTypeFilter } from "./lib/types";

export default function QuickActions({
  orgSlug,
  showCoachForm = false,
  showNutritionistForm = false,
  coachHref,
  nutritionistHref,
  onCreateSuccess,
}: {
  orgSlug: string;
  showCoachForm?: boolean;
  showNutritionistForm?: boolean;
  coachHref: string;
  nutritionistHref: string;
  onCreateSuccess?: (k: ProfessionalTypeFilter) => Promise<void> | void;
}) {
  return (
    <div>
      {showCoachForm ? (
        <CreateCoachForm orgSlug={orgSlug} onSuccess={() => onCreateSuccess?.("coach")} />
      ) : showNutritionistForm ? (
        <CreateNutritionistForm orgSlug={orgSlug} onSuccess={() => onCreateSuccess?.("nutritionist")} />
      ) : (
        <div className="space-y-6">
          <div className="border-border/70 shadow-sm rounded-lg">
            <div className="px-4 py-3">
              <h3 className="text-base font-semibold">Acciones rápidas</h3>
              <p className="text-sm text-muted-foreground">Gestiona altas y accesos sin salir de este panel.</p>
              <div className="mt-3 space-y-2">
                <Button asChild className="w-full">
                  <Link href={coachHref}>Crear coach</Link>
                </Button>
                <Button asChild variant="secondary" className="w-full">
                  <Link href={nutritionistHref}>Crear nutricionista</Link>
                </Button>
                <Button asChild variant="outline" className="w-full">
                  <Link href={`/${orgSlug}/members`}>Ir a miembros</Link>
                </Button>
              </div>
              <p className="mt-3 text-xs text-muted-foreground">Cada alta genera una invitación pendiente hasta que el profesional completa el onboarding.</p>
            </div>
          </div>
        </div>
      )}
    </div>
  );
}
